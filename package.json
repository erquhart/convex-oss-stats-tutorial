{
  "name": "convex-oss-stats-tutorial",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "shx rm convex.json && convex dev --tail-logs",
    "build": "tsc -b && vite build",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clear-db": "npx convex import --replace-all --table githubOwners --format jsonLines /dev/null -y",
    "step-dev:frontend": "vite",
    "step-dev:backend": "convex dev --tail-logs",
    "step-dev": "npm-run-all --parallel \"step-dev:frontend -- {1}\" step-dev:backend --",
    "step-01": "cd steps/step-01 && npm run dev:backend",
    "step-02": "cd steps/step-02 && npm run dev:backend",
    "step-03": "cd steps/step-03 && npm run dev:backend",
    "step-04": "cd steps/step-04 && npm run dev:backend",
    "step-05": "cd steps/step-05 && npm run dev:backend",
    "step-06": "cd steps/step-06 && npm run dev:backend",
    "step-07": "cd steps/step-07 && shx cp convex.json ../../convex.json && shx cp ../../.env.local . && npm run step-dev steps/step-07"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "convex": "^1.17.3",
    "convex-helpers": "^0.1.66",
    "octokit": "^4.0.2",
    "p-limit": "^6.1.0",
    "remeda": "^2.17.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "prettier": "3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "shx": "^0.3.4"
  }
}
